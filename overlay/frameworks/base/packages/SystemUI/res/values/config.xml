<resources>
    <dimen name="rounded_corner_content_padding">47.0px</dimen>

    <!-- Doze: check proximity sensor before pulsing? -->
    <bool name="doze_proximity_check_before_pulse">false</bool>

    <!-- Should we vibrate on an icon animation of the shelf. This should only be active if the
    vibrator is capable of subtle vibrations -->
    <bool name="config_vibrateOnIconAnimation">true</bool>

    <!-- Location on the screen of the center of the physical volume up/down buttons. -->
    <dimen name="physical_volume_up_button_center_screen_location_y">584px</dimen>
    <dimen name="physical_volume_down_button_center_screen_location_y">762px</dimen>

    <dimen name="physical_power_button_center_screen_location_y">1060px</dimen>
    <dimen name="physical_fingerprint_sensor_center_screen_location_y">1060px</dimen>
    
    <!-- Default rounded corner curve (a Bezier). Must match (the curved path in) rounded.xml.
         Note that while rounded.xml includes the entire path (including the horizontal and vertical
         corner edges), this pulls out just the curve.-->
    <string name="config_rounded_mask" translatable="false">M146,0 C100,2 60,0 30,30 C0,60 2,100 0,146</string>

    <bool name="config_roundedCornerMultipleRadius">true</bool>
</resources>
