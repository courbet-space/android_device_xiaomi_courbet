<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Default radius of the software rounded corners. -->
    <dimen name="config_rounded_mask_size">102px</dimen>    

    <!-- Radius of the software rounded corners at the bottom of the display
         in its natural orientation. If zero, the value of rounded_corner_radius is used. -->
    <dimen name="config_rounded_mask_size_top">102px</dimen>
    <dimen name="config_rounded_mask_size_bottom">102px</dimen>

    <!-- the padding on the top of the statusbar (usually 0) -->
    <dimen name="status_bar_padding_top">8dp</dimen>
    <dimen name="status_bar_padding_start">4dp</dimen>
    <dimen name="status_bar_padding_end">4dp</dimen>
</resources> 
